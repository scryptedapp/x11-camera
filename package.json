{
   "name": "@scrypted/x11-camera",
   "version": "0.0.2",
   "description": "X11 Virtual Cameras for Scrypted",
   "license": "Apache",
   "keywords": [
      "scrypted",
      "plugin",
      "virtual",
      "camera",
